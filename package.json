{
	"name": "int-transformer",
	"displayName": "Int Transformer",
	"description": "Transform int based code to double and long",
	"publisher": "wolray",
	"version": "1.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"homepage": "https://github.com/wolray/vscode-int-transformer",
	"keywords": [
		"java",
		"primitive",
		"generics"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wolray/vscode-int-transformer.git"
	},
	"activationEvents": [
		"onCommand:intTransformer.selection",
		"onCommand:intTransformer.file"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "intTransformer.selection",
				"title": "Int Transformer: Selection"
			},
			{
				"command": "intTransformer.file",
				"title": "Int Transformer: File"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
