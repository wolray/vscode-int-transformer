{
	"name": "int-transformer",
	"displayName": "Int Transformer",
	"description": "Generate code of primitive double and long from int-based source code",
	"publisher": "wolray",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.40.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:intTransformer.selection",
		"onCommand:intTransformer.file"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "intTransformer.selection",
				"title": "Transform int of selection"
			},
			{
				"command": "intTransformer.file",
				"title": "Transform int of file"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
